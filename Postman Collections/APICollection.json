							"var urlPath = pm.request.url.getHost();\r",
							"\r",
							"var ActualEnvUrl = pm.environment.get(\"url\");\r",
							"\r",
							"var GetCoreEnvUrl = ActualEnvUrl.split(\"//\");\r",
							"\r",
							"var CoreEnvUrl = GetCoreEnvUrl[1];\r",
							"\r",
							"pm.test(\"HostName is Same :\", function() {\r",
							"\r",
							"pm.expect(CoreEnvUrl).to.eql(urlPath);\r",
